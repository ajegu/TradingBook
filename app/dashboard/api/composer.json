{
    "name": "tradingbook/dashboard-api",
    "description": "API for TradingBook dashboard",
    "type": "application",
    "license": "MIT",
    "authors": [
        {
            "name": "Allan JEGU"
        }
    ],
    "autoload": {
        "psr-4": {
            "Ajegu\\TradingBook\\Dashboard\\Api\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ajegu\\TradingBook\\Dashboard\\Api\\Test\\": "tests/"
        }
    },
    "require": {
        "php": "^8.1",
        "ajegu/ddb-access": "dev-main",
        "laravel/lumen-framework": "^9.0"
    },
    "repositories": [
        {
            "type": "github",
            "url": "https://github.com/ajegu/ddb-access"
        }
    ],
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    }
}
